#function to return employee_of_the_month
def find_employee_of_the_month(emp):
    current_max = 0
    employee_of_the_month = ''
    for emp,hrs in emp:
        if hrs > current_max:
            current_max = hrs
            employee_of_the_month = emp
        else:
            pass

    return (employee_of_the_month,current_max)


